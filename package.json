{
  "name": "newweb",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    ".",
    "server"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:server": "cd server && npm run start:dev",
    "build": "next build",
    "build:server": "cd server && npm run build",
    "build:all": "npm run build && npm run build:server",
    "start": "next start",
    "start:server": "cd server && npm run start",
    "lint": "next lint",
    "lint:server": "cd server && npm run lint",
    "lint:all": "npm run lint && npm run lint:server",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:server": "cd server && npm run format",
    "format:all": "npm run format && npm run format:server",
    "fixx": "npm run format:all && npm run lint",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "type-check": "tsc --noEmit",
    "type-check:server": "cd server && tsc --noEmit",
    "type-check:all": "tsc --noEmit -p tsconfig.all.json",
    "tca": "npm run type-check:all",
    "ci": "npm install --workspaces",
    "i": "npm install",
    "i:b": "npm install --workspace=server"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.503.0",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-phone-number-input": "^3.4.12",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.25.1",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-next": "^0.0.0",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.31.0"
  }
}
